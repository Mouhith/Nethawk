    <div class="info-main-container">
      <div class="left-container">
        <div class="side-nav-links-div">
          <div class="blob1"></div>
          <div class="blob2"></div>
          <div class="blob3"></div>
          <!-- <div class="blob4"></div>
          <div class="blob5"></div> -->

          <img
            src="/images/Nuron white logo.png"
            alt="nuron-logo"
            class="nuron-logo"
          />
          <div class="links-container">
            <a class="side-nav-link" onClick="window.location.reload()">
              Introduction</a
            >
            <hr />
